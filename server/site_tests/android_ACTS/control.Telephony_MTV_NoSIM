AUTHOR = 'bettyzhou, jaineelm, krisr'
NAME = 'android_ACTS.Telephony_MTV_NoSIM'
TIME = 'MEDIUM'
TEST_TYPE = 'Server'
ATTRIBUTES = 'suite:android_bluetooth'
DEPENDENCIES = 'bluetoothcell'

DOC = """
This test runs telephony sanity against No SIM phones.
"""

def run(machine):
    job.run_test('android_ACTS',
                 testbed=hosts.create_testbed(machine),
                 config_file='android1758-bluetooth.config',
                 test_file='telephony_no_sim',
                 override_build_url='git_master-release/sailfish-userdebug/latest',
                 testtracker_project_id=11349,
                 testtracker_owner='android-comms-tvc@google.com',
                 testtracker_extra_env="no_sim_mtv",
                 acts_timeout=36000)

parallel_simple(run, machines)

