AUTHOR = 'sbasi'
NAME = 'brillo_AllowlistedGtests'
TIME = 'MEDIUM'
TEST_TYPE = 'Server'
ATTRIBUTES = 'suite:brillo-bvt,suite:brillo-smoke,suite:brillo-presubmit,suite:brillo-postsubmit'

DOC = """
This test runs all the allowlisted native gTests found on a Brillo DUT.

"""

def run(machine):
    job.run_test('brillo_Gtests', host=hosts.create_host(machine),
                 use_allowlist=True)


parallel_simple(run, machines)
