;
; Sequential write performance at the front of the disk
;

[seq_rw_verify]
filename=${FILENAME}
size=${FILESIZE}
verify_only=${VERIFY_ONLY}

time_based
runtime=5m

ioengine=libaio
iodepth=1
direct=1

readwrite=rw
rwmixread=10
bs=512k

do_verify=1
verify=md5
verify_dump=1
continue_on_error=${CONTINUE_ERRORS}